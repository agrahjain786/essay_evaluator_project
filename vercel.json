{
    "builds": [{
        "src": "essay_evaluator_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.1" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "essay_evaluator_project/wsgi.py"
        }
    ]
}